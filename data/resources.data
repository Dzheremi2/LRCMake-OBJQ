resPrefix:
  - name: Layouts
    resources:
      - name: ChronographWindow
        res: local:/data/ui/window.auis
        preprocess:
          use: qtk.layouts.auis.compile
  - name: Strings
    resources:
      - name: i18n_index
        res: local:/i18n/index.i18n
        preprocess:
          use: qtk.i18n.generate
          args: "--output=locales.i18n -merge-single-file"